    {{#each randomBeer}}
    <img src="{{this.image_url}}" alt="image">
    <h2>{{this.name}}</h2>
    <p><em>{{this.tagline}}</em></p>
    <p>{{this.description}}</p>
    <h3>Food Pairing</h3>
    <ul>
        {{#each this.food_pairing}}
        <li>{{this}}</li>
        {{/each}}
    </ul>
    <p><strong>Brewers Tips: </strong>{{this.brewers_tips}}</p>
    {{/each}}